{
  "name": "@jam/cli",
  "version": "0.0.1",
  "description": "a cli tool",
  "bin": {
    "jam": "bin/jam.js"
  },
  "main": "index.js",
  "scripts": {
    "update": "npm-check -u",
    "lint": "eslint --ext .js bin lib"
  },
  "author": "kitemao@gmail.com",
  "license": "ISC",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailing-comma": "es5"
  },
  "lint-staged": {
    "*.{js,md,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^5.0.5",
    "@types/inquirer": "0.0.44",
    "@types/node": "^11.10.4",
    "@types/semver": "^5.5.0",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "download-git-repo": "^1.1.0",
    "fs-extra": "^7.0.1",
    "inquirer": "^6.2.2",
    "semver": "^5.6.0"
  },
  "engines": {
    "node": ">=8.9"
  },
  "devDependencies": {
    "@types/eslint": "^4.16.6",
    "@types/prettier": "^1.16.1",
    "eslint": "^5.15.1",
    "husky": "^1.3.1",
    "npm-check": "^5.9.0",
    "prettier": "^1.16.4"
  }
}
